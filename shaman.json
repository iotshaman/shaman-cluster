{
	"name": "shaman-cluster",
	"projects": [
		{
			"name": "shaman-cluster-primary-api",
			"environment": "node",
			"template": "node-express-server",
			"path": "primary-server",
			"include": [
				"shaman-cluster-lib",
				"shaman-cluster-database"
			]
		},
		{
			"name": "shaman-cluster-secondary-api",
			"environment": "node",
			"template": "node-express-server",
			"path": "secondary-server",
			"include": [
				"shaman-cluster-lib",
				"service-bus"
			]
		},
		{
			"name": "shaman-cluster-database",
			"environment": "node",
			"template": "node-shaman-database",
			"path": "database"
		},
		{
			"name": "shaman-cluster-lib",
			"environment": "node",
			"template": "node-typescript-library",
			"path": "cluster-lib"
		},
		{
			"name": "service-bus",
			"environment": "node",
			"template": "node-typescript-library",
			"path": "service-bus"
		}
	],
	"transform": []
}